{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "blockylicious/clicky-group",
    "version": "0.1.0",
    "title": "Clicky Group",
    "category": "blockylicious_custom",
    "icon": "smiley",
    "description": "Custom buttons group. Buttons link to selected post or page rather than URL.",
    "example": {},
    "supports": {
        "html": false,
        "spacing": {
            "blockGap": true
        }
    },
    "attributes": {
        "justifyContent": {
            "type": "string",
            "enum": [
                "left",
                "center",
                "right"
            ],
            "default": "left"
        }
    },
    "textdomain": "blockylicious",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css",
    "render": "file:./render.php"
}